{
  "manifest_version": 3,
  "name": "a Better Place",
  "version": "1.1.1",
  "description": "Migliora l'esperienza utente dell'intranet aziendale",
  "permissions": [
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://*.it/*",
    "https://raw.githubusercontent.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "256": "icon.png"
  }
}